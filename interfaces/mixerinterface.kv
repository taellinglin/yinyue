<Channel>:
    orientation: "vertical"
    Label:
        text: root.channel_name
    GridLayout:
        cols: 1
        spacing: 10

<MixerInterface>:
    orientation: "horizontal"
    GridLayout:
        cols: 1
        size_hint: 0.7, 1
    GridLayout:
        cols: 1
        size_hint: 0.3, 1
